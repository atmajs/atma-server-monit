{
    "private": true,
    "scripts": {
        "build": "app-bundler"
    },
    "dependencies": {
        "alot": "^0.2.81",
        "axios": "^0.19.2",
        "compo-views": "^0.10.44",
        "includejs": "^0.15.43",
        "jquery": "^3.5.0",
        "maskjs": "^0.71.79",
        "material-components-web": "^6.0.0",
        "ruta": "^0.2.62"
    },
    "app-bundler": {
        "apps": {
            "mm": {
                "file": "index.dev.html",
                "outputMain": "index.html",
                "outputSources": "build/",
                "dynamicDependencies": []
            }
        },
        "common": {
            "package": {
                "module": "includejs"
            },
            "varDefs": {
                "DEBUG": false,
                "SERVER": true
            },
            "version": "${version}",
            "middlewares": {
                "less": [
                    "atma-loader-less:read"
                ],
                "ts": [
                    "atma-loader-ts:read"
                ]
            }
        }
    },
    "atma": {
        "plugins": [
            "atma-loader-ts",
            "atma-loader-less"
        ],
        "settings": {
            "include": {
                "amd": true,
                "extentionDefault": {
                    "js": "ts"
                }
            },
            "atma-loader-ts": {
                "typescript": {
                    "compilerOptions": {
                        "target": "es6",
                        "module": "AMD"
                    }
                }
            }
        }
    },
    "devDependencies": {
        "app-bundler": "0.0.88"
    }
}
